{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HttpClientSettingsDictionary": {
    "ICustomerApiClient": {
      "BaseUrl": "https://localhost:5001/",
      "RequestHeaders": {
        "Header-Test-001": "Testing Header"
      },
      "CircuitBreaker": {
        "Enabled": false,
        "BreakDurationInSeconds": 10,
        "EventsBeforeBreak": 3
      },
      "Retry": {
        "Enabled": false,
        "JitterEnabled": true,
        "MaxJitterInMiliseconds": 50,
        "NumberOfRetries": 3,
        "RetriesBackoff": "Linear",
        "SleepDurationInMiliseconds": 100
      },
      "Timeout": {
        "Enabled": false,
        "TimeoutInSeconds": 2
      }
    },
    "IAnotherApiClient": {
      "BaseUrl": "https://localhost:6001/",
      "RequestHeaders": {
        "Header-Test-001": "Testing Header"
      }
    }
  }
}
